
public enum Condiment {

	CREAM("cream", "Dairy based individual serving"), 
	SUGAR("sugar", "Case sugar teaspoon"), 
	NutraSweet("nutrasweet", "Individual serving");  
	
	private String name;
	private String description;
	
	private Condiment(String name, String description){
		
		this.name = name;
		this.description = description;
	}
}
